.container {
  background-color: #f0f0f0;
  min-height: 100%;
  display: block;
  z-index: 10;
}
/* -----头部导航栏----- */
.window-nav {
  height: 115px;
  background-color: #ff4f9c;
  border-bottom-left-radius: 50px;
  border-bottom-right-radius: 50px;
  z-index: -1;
}
.nav-head {
  padding: 30px 15px;
}
.logo {
  height: 18px;
  width: 68px;
}
.nav-search-form {
  border-radius: 15px;
  height: 24px;
  width: 170px;
  background-color: #f5f5f5;
  display: flex;
  align-items: center;
  margin-right: 30%;
  margin-bottom: 5px;
  text-align: center;
}
.body-size {
  width: 90%;
  margin: 0 auto;
  background-color: #f0f0f0;
}
.popular {
  background-color: #ffffff;
}
.txt {
  height: 42rpx;
  line-height: 42rpx;
  color: #666;
  padding-left: 40px;
  font-size: 30rpx;
}
/* --------- */
.cuIcon-search {
  padding-right: 10px;
}
.category-box {
  background-color: #fff;
  clear: both;
}
.category-list {
  width: 150rpx;
  margin-top: 20rpx;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}
.category-column {
  width: 100%;
  margin-top: 20rpx;
  overflow: hidden;
}
.category-imgbox {
  width: 60rpx;
  height: 60rpx;
}
.category-title {
  font-size: 24rpx;
  text-align: center;
  margin-top: 10rpx;
}

.swiper-container {
  position: relative;
  top: -30px;
  z-index: 10;
}
.swiper_box {
  width: 750rpx;
  height: 250rpx;
}

swiper-item image {
  width: 90%;
  height: 125px;
  border-radius: 5px;
}
.swiper-container .dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: -15px;
  display: flex;
  justify-content: center;
}
.swiper-container .dots .dot {
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: #bfbfbf;
  border-radius: 50%;
  transition: all 0.6s;
  opacity: 0.5;
}
.swiper-container .dots .dot.active {
  width: 14rpx;
  opacity: 1;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.type-item-on {
  color: #e64340;
  border-bottom: 1rpx solid #e64340;
}
.weui-loadmore {
  margin-bottom: 0px;
}
.goods-container {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  box-sizing: content-box;
}
.goods-box {
  background-color: #ffffff;
  width: 48%;
  height: 200px;
  overflow: hidden;
  padding: 14px 15px;
  margin-bottom: 10px;
  border-radius: 5px;
}
.goods-box .img-box {
  width: 131px;
  height: 131px;
  overflow: hidden;
}
.goods-box .img-box image {
  width: 131px;
  height: 131px;
}
.goods-box .goods-title {
  width: 280rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  font-size: 24rpx;
  padding-top: 12rpx;
  color: #161616;
}
.goods-box .goods-price {
  width: 280rpx;
  overflow: hidden;
  font-size: 24rpx;
  padding: 12rpx 0;
  color: #e4007f;
}
.no-more-goods {
  text-align: center;
  font-size: 24rpx;
  padding-bottom: 48rpx;
  color: #999;
}
.notice {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  height: 80rpx;
  line-height: 80rpx;
  width: 750rpx;
  margin-top: 10rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #efeff4;
}
.notice_icon {
  margin-top: 25rpx;
  margin-left: 20rpx;
  width: 30rpx;
  height: 30rpx;
}
.notice_swiper {
  height: 80rpx;
  line-height: 80rpx;
  width: 680rpx;
}
.notice_itemr {
  padding-left: 10rpx;
  font-size: 24rpx;
  overflow: hidden;
  color: #e64340;
  width: 680rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.pos-fiexd {
  position: fixed;
  top: 0;
  left: 0;
}
.coupons {
  margin-top: 10rpx;
  width: 100%;
  height: 180rpx;
  overflow: hidden;
}
.coupons-scroll {
  white-space: nowrap;
  height: 180rpx;
  width: 100%;
  background-color: #fff;
}
.coupons-item {
  width: 300rpx;
  height: 180rpx;
  margin: 10rpx;
  padding-top: 20rpx;
  padding-left: 15rpx;
  background-color: #f1a83b;
  box-sizing: content-box;
  font-size: 20rpx;
  line-height: 35rpx;
  overflow: hidden;
  color: #fff;
  display: inline-block;
}
.coupons-float {
  position: fixed;
  right: 15rpx;
  bottom: 80rpx;
  width: 80rpx;
  height: 80rpx;
  background-color: #ffffff;
  opacity: 0.8;
  text-align: center;
  border-radius: 50%;
  border: 1rpx solid #ddd;
}
.forum-float {
  position: fixed;
  right: 15rpx;
  bottom: 180rpx;
  width: 80rpx;
  height: 80rpx;
  background-color: #ffffff;
  opacity: 0.8;
  text-align: center;
  border-radius: 50%;
  border: 1rpx solid #ddd;
}
.forum-float image {
  width: 60rpx;
  height: 60rpx;
  margin-top: 10rpx;
}
.coupons-float image {
  width: 60rpx;
  height: 60rpx;
  margin-top: 10rpx;
}
.tuan {
  width: 750rpx;
  background-color: #f2f2f2;
  padding-top: 10rpx;
}
.tuan-item {
  width: 720rpx;
  margin: auto;
  margin-top: 20rpx;
  background-color: #fff;
  position: relative;
}
.tuan-goods-pic {
  width: 720rpx;
  height: 250rpx;
  overflow: hidden;
}
.tuan-goods-pic image {
  width: 720rpx;
}
.tuan-title {
  margin-top: 20rpx;
  margin-left: 20rpx;
  font-size: 30rpx;
  font-weight: 400;
}
.tuan-profile {
  margin-left: 20rpx;
  font-size: 28rpx;
  color: #999;
}
.tuan-price {
  margin-left: 20rpx;
}
.tuan-price .now {
  font-size: 36rpx;
  color: #e64340;
}
.tuan-price .original {
  margin-left: 20rpx;
  font-size: 26rpx;
  color: #999;
  text-decoration: line-through;
}
.tuan-btn {
  position: absolute;
  right: 30rpx;
  bottom: 10rpx;
}
